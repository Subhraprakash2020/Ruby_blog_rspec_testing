 <div class="container">
  <div class="card2 register">
  <h2>Login</h2>
<%= form_with url: login_path, method: :post do |form| %>
  <div>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </div>

  <div>
    <%= form.label :password %><br>
    <%= form.password_field :password %>
  </div>

  <div class = "fsub">
    <%= form.submit "Login"%>
  </div>
<% end %>
</div>
</div>
